<p id="notice"><%= notice %></p>

<h1>Listing Patients</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone</th>
      <th>Medicare number</th>
      <th>Street address</th>
      <th>Suburb</th>
      <th>State</th>
      <th>Postcode</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @patients.each do |patient| %>
      <tr>
        <td><%= patient.user %></td>
        <td><%= patient.first_name %></td>
        <td><%= patient.last_name %></td>
        <td><%= patient.phone %></td>
        <td><%= patient.medicare_number %></td>
        <td><%= patient.street_address %></td>
        <td><%= patient.suburb %></td>
        <td><%= patient.state %></td>
        <td><%= patient.postcode %></td>
        <td><%= patient.latitude %></td>
        <td><%= patient.longitude %></td>
        <td><%= link_to 'Show', patient %></td>
        <td><%= link_to 'Edit', edit_patient_path(patient) %></td>
        <td><%= link_to 'Destroy', patient, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Patient', new_patient_path %>
